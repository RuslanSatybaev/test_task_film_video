<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="testgroup.filmograhy.controller.FilmTop" table="films">
        <meta attribute="class-description">
            This class contains developer's details.
        </meta>
        <id name="id" type="int" column="ID">
            <generator class="native"/>
        </id>
        <property name="rating" column="rating" type="double"/>
        <property name="nameEn" column="title" type="string"/>
        <property name="year" column="year" type="int"/>
        <property name="ratingVoteCount" column="countVotePeople" type="int"/>
    </class>
</hibernate-mapping>